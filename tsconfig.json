{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "build",
    "baseUrl": "src",
    "strict": true,
    "noImplicitAny": false,
    "sourceMap": true,
    "declaration": true,
    "moduleResolution": "node",
    "typeRoots": [
      "node_modules/@types"
    ],
    "target": "es5",
    "lib": [
      "es2016"
    ]
  },
  "include": [
    "src"
  ]
}
